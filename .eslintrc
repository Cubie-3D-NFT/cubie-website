{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "extends": ["prettier/react"],
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "React": true,
    "document": true,
    "window": true,
    "jQuery": true,
    "$": true,
    "localStorage": true,
    "fetch": true
  },
  "rules": {
    "default-case": 0,
    "no-console": 0,
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0
  }
}
